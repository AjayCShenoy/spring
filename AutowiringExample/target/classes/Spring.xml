<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans"
xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation = "http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

<bean id="author" class="com.ust.spring.AutowiringExample.Author">
<property name="id" value="1"></property>
<property name="firstname" value="Ajay"></property>
<property name="lastname" value="CM"></property>
</bean>
<bean id="book" class="com.ust.spring.AutowiringExample.Book" autowire="byType">
<property name="isbn" value="12345678"></property>
<property name="title" value="you can win"></property>
</bean>

</beans>